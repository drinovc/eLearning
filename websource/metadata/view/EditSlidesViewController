            "reference": {
                "name": "items",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "fn": "getNumSlides",
                "implHandler": [
                    "// Retrieves count of slides (sections don't count)",
                    "",
                    "var countSlides = 0;",
                    "var data = localStorage.getItem('mxp_elearning_slide');",
                    "if(data) {",
                    "    data = Ext.decode(data);",
