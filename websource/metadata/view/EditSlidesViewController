                    "    answers = opts.answers,",
                    "    multi = opts.multi,",
                    "    component = opts.component;",
                    "",
                    "if(answers.length === 0) {",
                    "    return 'Please specify at least one answer';",
                    "}",
                    "if(answers.filter(function(item) { return item.correct; }).length === 0) {",
