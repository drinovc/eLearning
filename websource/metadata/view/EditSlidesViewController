        {
            "type": "basicfunction",
            "reference": {
                "name": "items",
                "type": "array"
            },
            "codeClass": null,
            "userConfig": {
                "designer|params": [
                    "storeName",
                    "localStorageAttribute",
                    "LoadParams",
                    "callback",
                    "dataId"
                ],
                "fn": "initialDataSync",
                "implHandler": [
                    "if (!navigator.onLine){",
                    "\t//You are offline - cannot sync with server",
                    "\tif(callback){",
                    "\t\tcallback();",
                    "\t}",
                    "\treturn;",
                    "}",
                    "",
                    "var me = this,",
                    "\tlocalStorageData = Ext.decode(localStorage.getItem('mxp_elearning')),",
                    "",
                    "\tinitialDataStore = me.getStore(storeName);",
                    "",
                    "initialDataStore.load({",
                    "\tparams: LoadParams,",
                    "",
                    "\tcallback: function(records, operation, success){",
                    "\t\tif(initialDataStore.data.length === 0){",
                    "\t\t\t// no return data, check if there are any locally saved localStorageAttribute",
                    "",
                    "\t\t\tif(localStorageData && localStorageData[me.programId] && localStorageData[me.programId][localStorageAttribute]){",
                    "\t\t\t\t//we have locally saved localStorageAttribute, syncing with server",
                    "\t\t\t\t// data for this programId exists - reassign it",
                    "\t\t\t\tlocalStorageData = localStorageData[me.programId];",
                    "\t\t\t\tfor (var key in localStorageData[localStorageAttribute]){",
                    "\t\t\t\t\tvar localStorageEntry = localStorageData[localStorageAttribute][key];",
                    "",
                    "\t\t\t\t\tif(localStorageAttribute == 'slides'){",
                    "\t\t\t\t\t\t// we must append this slide not add",
                    "\t\t\t\t\t\tinitialDataStore.getRootNode().appendChild(localStorageEntry);",
                    "\t\t\t\t\t\tinitialDataStore.findRecord('id', localStorageEntry.id).phantom = true;",
                    "\t\t\t\t\t}",
                    "\t\t\t\t\telse{",
                    "\t\t\t\t\t\tvar rec = initialDataStore.add(localStorageEntry)[0];",
                    "\t\t\t\t\t\trec.phantom = true;",
                    "\t\t\t\t\t}",
                    "\t\t\t\t}",
                    "\t\t\t}",
                    "\t\t\tif(callback){ callback();}",
                    "\t\t\treturn;",
                    "\t\t}",
                    "",
                    "\t\t// here we have valid return data from server, so we can update localstorage and sync localstorage back with server",
                    "\t\tif(!localStorageData || !localStorageData[me.programId]){",
                    "\t\t\t// no data yet in localstorage",
                    "\t\t\tlocalStorageData = me.getCurrentState();",
                    "\t\t\tinitialDataStore.data.items.forEach(function(entry){",
                    "\t\t\t\tlocalStorageData[localStorageAttribute].push(entry.data);",
                    "\t\t\t});",
                    "\t\t\tif(callback){ callback(); }",
                    "\t\t\treturn;",
                    "\t\t}",
                    "",
                    "\t\t// data for this programId exists - reassign it",
                    "\t\tlocalStorageData = localStorageData[me.programId];",
                    "",
                    "\t\tif(!localStorageData[localStorageAttribute]){",
                    "\t\t\t//no entries yet in localstorage",
                    "\t\t\t// localstorage slides are empty - set recieved data from server to localstorage - even if it is empty",
                    "",
                    "\t\t\tvar entriessFromStore = {};",
                    "\t\t\tfor (var i = 0; i < initialDataStore.data.length; i++){",
                    "\t\t\t\tvar entry = initialDataStore.data.items[i].data;",
                    "\t\t\t\tentriessFromStore[entry[dataId]] = entry;",
                    "\t\t\t}",
                    "",
                    "\t\t\t// add localStorageAttribute from store to localstorage[programId] under localStorageAttribute key",
                    "\t\t\tlocalStorageData[localStorageAttribute] = entriessFromStore;",
                    "",
                    "\t\t\tif(callback){ callback(); }",
                    "\t\t\treturn;",
                    "\t\t}",
                    "",
                    "\t\tfunction containsObject(obj, list) {",
                    "\t\t\tfor (var i = 0; i < list.length; i++) {",
                    "\t\t\t\tif (list[i].id == obj.id) {",
                    "\t\t\t\t\treturn i;",
                    "\t\t\t\t}",
                    "\t\t\t}",
                    "\t\t\treturn -1;",
                    "\t\t}",
                    "",
                    "\t\t// we dont have to worry about deleted components/slides",
                    "",
                    "\t\tfor (var key in localStorageData[localStorageAttribute]){",
                    "\t\t\tvar localStorageEntry = localStorageData[localStorageAttribute][key];",
                    "\t\t\tvar objectIndex = containsObject(localStorageEntry, initialDataStore.data.items); // get index of this item",
                    "\t\t\tif(objectIndex > -1) { // if returned index 0,1,2..",
                    "\t\t\t\t// we found localstorage entry in store - update it",
                    "",
                    "\t\t\t\tif(localStorageEntry.lastChanged > initialDataStore.data.items[objectIndex].data.lastChanged){",
                    "\t\t\t\t\t// update entry in",
                    "\t\t\t\t\tvar rec = initialDataStore.findRecord('id', localStorageEntry.id);",
                    "\t\t\t\t\trec.data = localStorageEntry;",
                    "\t\t\t\t\trec.phantom = true;",
                    "\t\t\t\t}",
                    "\t\t\t}else{",
                    "\t\t\t\tif(localStorageAttribute == 'slides'){",
                    "\t\t\t\t\t// for tree store, we add data to it by appending it to root node",
                    "\t\t\t\t\tinitialDataStore.getRootNode().appendChild(localStorageEntry);",
                    "\t\t\t\t\tinitialDataStore.findRecord('id', localStorageEntry.id).phantom = true;",
                    "\t\t\t\t}",
                    "\t\t\t\telse{",
                    "\t\t\t\t\t// for all other stores - that are json stores we can add entry normally",
                    "\t\t\t\t\tvar rec = initialDataStore.add(localStorageEntry)[0];",
                    "\t\t\t\t\trec.phantom = true;",
                    "\t\t\t\t}",
                    "\t\t\t}",
                    "\t\t}",
                    "\t\t// dont save state here - it will overwrite all localstorage data that hasnt been synced yet with server",
                    "",
                    "\t\tif(callback){ callback(); }",
                    "\t}",
                    "});",
                    "",
                    ""
                ]
