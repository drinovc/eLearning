                    "",
                    "        if(REC_TO_SLIDE){",
                    "            // recursively iterate to leaf slide through section",
                    "            if (this.getNumSlides() === 0){",
                    "                //cannot switch recursively to next slide\", \"no slides exist ",
                    "                return;",
                    "            }",
